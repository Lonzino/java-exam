package ch06;

public class ex06_2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] sales = new int[5];
		
		sales[0] = 52;
		sales[1] = 50;
		sales[2] = 52;
		sales[3] = 54;
		sales[4] = 57;
		
		int sum = sales[0] + sales[1] +sales[2] +sales[3]+sales[4];
		System.out.printf("총 판매량: %d잔", sum);
		
	
	}

}
