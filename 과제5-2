package exam;

public class Ch5AND2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 5;
		
		for (int i =0; i < num; i++) {
			for(int j = num; j > i; j--) {
				System.out.print(" ");
			}
			for(int K = I*2+1; K > 0; K--) {
				System.out.print("*");
			}
			System.out.println();
		}
	}

}
