package ch08;

public class accounttest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		account acc = new account();
		acc.setbalance(1000);
		System.out.printf("잔액: %d", getbalance);
	}

	
}

class account{
	private int balance;
	
	public int getbalance() {
		return balance;
	}
	public void setbalance(int b) {
		balance = b;
	}
}
